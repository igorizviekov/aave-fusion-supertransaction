{
	"name": "aave-fusion-supertransaction",
	"version": "1.0.0",
	"description": "Biconomy MEE AAVE Fusion Supertransaction Demo",
	"main": "index.js",
	"scripts": {
		"setup": "npm install && cp env.example .env",
		"setup:mee": "bash ./scripts/setup-mee-node.sh",
		"start:anvil": "bash ./scripts/start-anvil.sh",
		"start:mee": "cd mee-node-deployment && docker compose --env-file ../.env up -d",
		"stop:mee": "cd mee-node-deployment && docker compose down",
		"fund:account": "tsx scripts/fund-account.ts",
		"check:balance": "tsx scripts/check-balance.ts",
		"init:abstractjs": "tsx scripts/init-abstractjs.ts",
		"clean": "rm -rf dist node_modules .env"
	},
	"author": "igorizviekov",
	"license": "MIT",
	"dependencies": {
		"@biconomy/abstractjs": "^1.1.9",
		"axios": "^1.6.2",
		"dotenv": "^16.3.1",
		"ethers": "^6.8.1",
		"viem": "^2.37.12"
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"tsx": "^4.6.0",
		"typescript": "^5.3.2"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
